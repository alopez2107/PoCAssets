        "5f933faf-7b02-4781-934a-7cb6c0e97ae9": {
          "_id": "5f933faf-7b02-4781-934a-7cb6c0e97ae9",
          "name": "createSupplierOrg",
          "description": "Display a form to create a new Supplier Organization",
          "script": "\"/*\\n  - Data made available by nodes that have already executed are available in the sharedState variable.\\n  - The script should set outcome to either \\\"true\\\" or \\\"false\\\".\\n */\\nvar supplierTypes = openidm.query(\\\"managed/SupplierType\\\", {_queryFilter: \\\"true\\\"}, [\\\"*\\\"]).result;\\n\\nvar supplierTypeOptions = [\\\"Select a Type of Supplier\\\"];\\nsupplierTypes.forEach((supplierType) => {\\n    supplierTypeOptions.push(supplierType.name);\\n})\\nvar fields = [\\n    \\\"supplierNumber\\\",\\n    \\\"name\\\",\\n    \\\"description\\\"\\n];\\n\\nif (callbacks.isEmpty()) {\\n    callbacksBuilder.stringAttributeInputCallback(\\\"supplierNumber\\\", \\\"Supplier Number\\\", \\\"\\\", true);\\n    callbacksBuilder.stringAttributeInputCallback(\\\"name\\\", \\\"Supplier Name\\\", \\\"\\\", true);\\n    callbacksBuilder.stringAttributeInputCallback(\\\"description\\\", \\\"Description\\\", \\\"\\\", true);\\n    callbacksBuilder.choiceCallback(\\\"Please Select your Supplier Organization Type\\\", supplierTypeOptions, 0, false);\\n}\\nelse {\\n    var selectedType = callbacks.getChoiceCallbacks().get(0)[0];\\n    var stringAttrInputCallbacks = callbacks.getStringAttributeInputCallbacks();\\n    var index = 0;\\n    stringAttrInputCallbacks.forEach((callback) => {\\n       logger.error(\\\"Callback found {} for Field name {}\\\", callback, fields[index]);\\n       nodeState.putShared(fields[index], callback); \\n        ++index;\\n    });\\n\\n    var selectedTypeID = supplierTypes[selectedType-1].id;\\n    logger.error(\\\"New Supplier Org data {} {} {} {}\\\", nodeState.get(\\\"name\\\"), nodeState.get(\\\"supplierNumber\\\"), nodeState.get(\\\"description\\\"), selectedTypeID);\\n    var newSupplierOrg = {\\n        name: nodeState.get(\\\"name\\\"),\\n        supplierNumber: nodeState.get(\\\"supplierNumber\\\"),\\n        description: nodeState.get(\\\"description\\\"),\\n        supplierTypeID: selectedTypeID\\n    };\\n\\n    try {\\n        var newSupplierOrgObj = openidm.create(\\\"managed/alpha_organization\\\", null, newSupplierOrg);\\n        action.goTo(\\\"true\\\");    \\n    }\\n    catch (e) {\\n        logger.error(\\\"Exception caught {}\\\", e);\\n        action.goTo(\\\"false\\\");\\n    }\\n} \\n\\n\"",
