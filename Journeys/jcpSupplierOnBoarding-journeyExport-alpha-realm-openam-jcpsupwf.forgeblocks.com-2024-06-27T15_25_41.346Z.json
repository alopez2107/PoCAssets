        "ee08a4e6-85cd-4fee-910b-50beb5340a27": {
          "_id": "ee08a4e6-85cd-4fee-910b-50beb5340a27",
          "name": "verifySupplierAdmin",
          "description": "Verifies that the logged in user is in the Supplier Admins internal role.",
          "script": "\"/*\\n  - Data made available by nodes that have already executed are available in the sharedState variable.\\n  - The script should set outcome to either \\\"true\\\" or \\\"false\\\".\\n */\\n\\nvar _id = nodeState.get(\\\"_id\\\");\\nvar userEndpoint = \\\"managed/alpha_user\\\";\\n\\nvar authnUser = nodeState.getObject(\\\"authnUser\\\");\\nif (!authnUser) {\\n    var userName = nodeState.get(\\\"username\\\");\\n    logger.error(\\\"The userName is {}\\\", userName);\\n    var _queryFilter = `userName eq \\\"${userName}\\\"`;\\n    authnUser = openidm.query(userEndpoint, {\\\"_queryFilter\\\": _queryFilter}, [\\\"*\\\", \\\"*_ref/*/*\\\"]).result[0];\\n}     \\n    \\nvar orgOptions = [];\\nvar authzRoles = authnUser.authzRoles;\\nvar userType = \\\"noadmin\\\";\\n\\nvar cleanObjectAttributes = {\\n    userName: \\\"\\\",\\n    givenName: \\\"\\\",\\n    sn: \\\"\\\",\\n    mail: \\\"\\\",\\n    frIndexedString2: \\\"\\\",\\n    frUnindexedString1: \\\"\\\",\\n    telephoneNumber: \\\"\\\"\\n};\\n\\nauthzRoles.forEach((role) => {\\n    logger.error(\\\"The authz role found is {}\\\", JSON.stringify(role));\\n    if (role.name === \\\"Supplier Admins\\\") {\\n        userType = \\\"jcpAdmin\\\";\\n    } \\n    if (role.name.includes(\\\"Registrars\\\")) {\\n        cleanObjectAttributes[\\\"frIndexedString2\\\"] = authnUser.frIndexedString2;  // Pre-fill the supplierNumber from the Registrar\\n        userType = \\\"registrar\\\";\\n    }\\n});\\nnodeState.putShared(\\\"authnUser\\\", authnUser);\\nnodeState.putShared(\\\"objectAttributes\\\", cleanObjectAttributes);\\naction.goTo(userType);\\n\"",
          "default": false,
          "language": "JAVASCRIPT",
